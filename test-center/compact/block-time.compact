import CompactStandardLibrary;

export circuit testBlockTimeLt(time: Uint<64>): Boolean {
  return blockTimeLt(disclose(time));
}

export circuit testBlockTimeGte(time: Uint<64>): Boolean {
  return blockTimeGte(disclose(time));
}

export circuit testBlockTimeGt(time: Uint<64>): Boolean {
  return blockTimeGt(disclose(time));
}

export circuit testBlockTimeLte(time: Uint<64>): Boolean {
  return blockTimeLte(disclose(time));
}