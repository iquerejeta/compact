{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ES2021"],
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,

    "outDir": "out"
  }
}
