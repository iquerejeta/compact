{
  "name": "compact",
  "displayName": "Compact",
  "description": "Compact Language Support",
  "publisher": "midnightnetwork",
  "license": "Apache-2.0",
  "version": "0.2.11",
  "engines": {
    "vscode": "^1.100.0"
  },
  "icon": "icon-white.png",
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.createCompactcProjectFromTemplate"
  ],
  "main": "./dist/extension",
  "contributes": {
    "languages": [
      {
        "id": "compact",
        "aliases": [
          "compact"
        ],
        "extensions": [
          ".compact"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "logo-black.png",
          "dark": "logo-white.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "compact",
        "scopeName": "source.compact",
        "path": "./syntaxes/compact.tmLanguage.json"
      }
    ],
    "problemMatchers": [
      {
        "name": "compactException",
        "owner": "compact",
        "fileLocation": [
          "absolute"
        ],
        "pattern": {
          "regexp": "^Exception: (.*?) line (\\d+), char (\\d+): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      },
      {
        "name": "compactInternal",
        "owner": "compact",
        "fileLocation": [
          "absolute"
        ],
        "pattern": {
          "regexp": "^(Exception in (.*?): \\(internal error\\) (.*?))$",
          "message": 1
        }
      },
      {
        "name": "compactCommandNotFound",
        "owner": "compact",
        "fileLocation": [
          "absolute"
        ],
        "pattern": {
          "regexp": "^((.*?) command not found: (.*?))$",
          "message": 1
        }
      }
    ],
    "breakpoints": [
      {
        "language": "compact"
      }
    ],
    "snippets": [
      {
        "language": "compact",
        "path": "compact.code-snippets"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "github.com:midnightntwrk/compactc.git"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.4.2",
    "chai": "^6.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.19.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-promise": "^7.2.1",
    "glob": "^11.0.2",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jest-silent-reporter": "^0.6.0",
    "mocha": "^11.3.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "package": "yarn build && vsce package --no-yarn",
    "clean": "rimraf out dist reports coverage",
    "clean-all": "rimraf out dist coverage reports node_modules *.vsix",
    "compile": "copyfiles ./tests/resources/* ./out && tsc -b",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --forceExit",
    "test-it": "rimraf out && yarn compile && node ./out/tests/runIntegrationTests.js",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint ./src/**/*.ts ./tests/**/*.ts",
    "format": "prettier --ignore-path ../../../.gitignore --write \"**/*.+(ts)\""
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
