import CompactStandardLibrary;

// test: check if we can still use methods from CompactStandardLibrary
export circuit nativeMethods(): [] {
  const a: Field = transientHash<Field>(default<Field>);
  const b: Field = transientCommit<Field>(default<Field>, default<Field>);
  const c: Bytes<32> = persistentHash<Field>(default<Field>);
  const d: Bytes<32> = persistentCommit<Field>(default<Field>, default<Bytes<32>>);
  const e: Field = degradeToTransient(default<Bytes<32>>);
  const f: Bytes<32> = upgradeFromTransient(default<Field>);
  const g: JubjubPoint = ecAdd(default<JubjubPoint>, default<JubjubPoint>);
  const h: JubjubPoint = ecMul(default<JubjubPoint>, default<Field>);
  const i: JubjubPoint = ecMulGenerator(default<Field>);
  const j: JubjubPoint = hashToCurve<Field>(default<Field>);
  const k: ZswapCoinPublicKey = ownPublicKey();
  const l: [] = createZswapInput(default<QualifiedShieldedCoinInfo>);
  const m: [] = createZswapOutput(default<ShieldedCoinInfo>, default<Either<ZswapCoinPublicKey, ContractAddress>>);
}
