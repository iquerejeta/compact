module T {
  module T1 {
    export witness test2(a: Field): Field;
  }
  
  import { test2 } from T1;
  export { test2 };
}

import { test2 } from T;

circuit test(): Field {
   const a = test2(1);
   return a;
}

export circuit test1(a: Field): Field {
   const a = disclose(test2(1));
   return a;
}