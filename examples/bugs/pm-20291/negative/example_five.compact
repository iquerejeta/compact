// test18: same signature, enum return types
enum Status { Success, Failure, Pending }
enum Color { Red, Green, Blue }
enum Direction { North, South, East, West }

module M18 {
  export circuit test18(n: Field): Status { return Status.Success; }
  export circuit test18(n: Field): Color { return Color.Red; }
  export circuit test18(n: Field): Direction { return Direction.North; }
  export circuit call18a(): Status { return test18(1); }
  export circuit call18b(): Color { return test18(1); }
  export circuit call18c(): Direction { return test18(1); }
}

import M18;
export { call18a, call18b, call18c };