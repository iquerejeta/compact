import CompactStandardLibrary;

ledger c: List<QualifiedCoinInfo>;

export circuit reset_to_default(): [] {
  return c.resetToDefault();
}

export circuit head(): Maybe<QualifiedCoinInfo> {
  return c.head();
}

export circuit is_empty(): Boolean {
  return c.isEmpty();
}

export circuit length(): Uint<64> {
  return c.length();
}

export circuit pop_front(): [] {
  return c.popFront();
}

export circuit push_front(value: QualifiedCoinInfo): [] {
  return c.pushFront(disclose(value));
}
