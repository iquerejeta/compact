import CompactStandardLibrary;

export circuit test(): [] {
  // mint
  const k = kernel.self();
  const domain_sep = pad(32, "qa_token");
  const amount = 1;
  kernel.mintShielded(domain_sep, amount);

  // checkpoint
  kernel.checkpoint();

  const note = pad(32, "note");
  const nul = pad(32, "nul");
  const addr = k.bytes;
  const entry_point = pad(32, "entry");
  const comm = 1;

  kernel.claimZswapCoinReceive(note);
  kernel.claimZswapCoinSpend(note);
  kernel.claimZswapNullifier(nul);
  kernel.claimContractCall(addr, entry_point, comm);
}
