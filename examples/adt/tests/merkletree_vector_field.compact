import CompactStandardLibrary;

export ledger mt: MerkleTree<2, Vector<64, Field>>;

constructor() {
  const v = default<Vector<64, Field>>;
  for (const i of 0..4) {
    mt.insert(v);
  }
}

export circuit test(): [] {
  assert(mt.isFull() == true, "Should be full");
  mt.resetToDefault();
  assert(mt.isFull() == false, "Should not be full");
}
