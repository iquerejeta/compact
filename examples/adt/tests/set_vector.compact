import CompactStandardLibrary;

export ledger c: Set<Vector<2, Field>>;

circuit getVector(): Vector<2, Field> {
  return [2, 12];
}

export circuit test(): [] {
  const zero = default<Vector<2, Field>>;
  const one = getVector();

  assert(c.isEmpty() == true, "Structure should be empty");
  assert(c.size() == 0, "Size should be 0");

  c.insert(zero);
  assert(c.size() == 1, "Size should be 1");
  assert(c.member(zero) == true, "Key 0 should have value 1");

  c.insert(one);
  assert(c.size() == 2, "Size should be 2");
  assert(c.member(one) == true, "Key 1 should be member");

  c.remove(zero);
  assert(c.size() == 1, "Size should be 1");
  assert(c.member(one) == true, "Key 1 should be member");

  c.resetToDefault();
  assert(c.size() == 0, "Size should be 0");
  assert(c.isEmpty() == true, "Structure should be empty");
}
